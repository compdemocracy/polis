{
  "name": "polis-client-admin",
  "version": "0.0.1",
  "repository": "github:pol-is/polisClientAdmin",
  "description": "Polis Admin Console",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "build:webpack_unminified": "npm run clean && NODE_ENV=development webpack --config webpack.config.unminified.js",
    "build": "npm run build:webpack",
    "build:preprod": "gulp deployPreprod",
    "build-debug": "npm run build:webpack_unminified",
    "deploy:prod": "./deploy_TO_PRODUCTION",
    "deploy:preprod": "./deployPreprod",
    "start": "./x",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest"
  },
  "author": "Colin Megill",
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@mdx-js/loader": "^1.6.22",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "bluebird": "^3.7.2",
    "compression-webpack-plugin": "^7.1.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "file-loader": "~6.2.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.0",
    "gulp-cli": "^1.4.0",
    "gulp-s3": "github:pol-is/gulp-s3#master",
    "gulp-scp2": "^0.2.0",
    "lodash-webpack-plugin": "^0.11.6",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "run-sequence": "~2.2.1",
    "webpack": "^5.24.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-middleware": "^4.1.0"
  },
  "dependencies": {
    "boolean": "^3.0.2",
    "d3-scale": "^3.2.3",
    "jquery": "^2.1.4",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-easy-emoji": "^1.4.0",
    "react-redux": "7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "theme-ui": "^0.3.5",
    "victory": "^35.4.9"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": "src/**/*.js",
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ]
  }
}
